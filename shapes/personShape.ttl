@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix schema: <https://schema.org/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

schema:PersonShape
    a sh:NodeShape ;
    sh:targetClass schema:Person ;

    sh:property [
        sh:path rdfs:label ;
        sh:minCount 1 ;
    ] ;

    sh:property [
        sh:path schema:url ;
        sh:minCount 1 ;
    ] .
